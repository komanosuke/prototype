                      <div><%= image_tag "/ctrlpanel/icon_shortcut_check.svg" %></div>
                      <div class="products_icon"><%= image_tag "/products/icon_bench_error.svg" %></div>
                      <div><%= @bench.name %><br>スマートベンチ</div>
                      <div><%= @bench.os_name %></div>
                      <div><%= @bench.introduced_date %></div>
                      <div><%= @bench.position %><br><object><a href="https://goo.gl/maps/pLPTXNQhMVX8WUdu5" target="_blank">Mapで確認する</a></object></div>
                    </div>
                  </a>
                  <div class="popup_btn">…</div>
                  <div class="popup_window">
                    <span class="popup_close">×</span>
                    <ul>
                      <li class="edit_btn">
                        <a href="">編集する</a>
                      </li>
                      <li data-action="confirm_delete" class="delete_btn">削除する</li>
                    </ul>
                  </div>
                </li>
                <li class="item">
                  <a href="#">
                    <div class="item_element flex_container">
                      <div><%= image_tag "/ctrlpanel/icon_shortcut_check.svg" %></div>
