        <div class="content_frame">
          <div class="products_ttl flex_container">
            <h2>プロダクト一覧</h2>
            <div class="content_btn flex_container_s">
              <div class="select">
                <%= image_tag "/common/icon_filter.svg" %>
                <select name="filter">
                  <option value="default">絞り込み</option>
                  <option value="">スマートベンチ</option>
                  <option value="">ライブカメラ</option>
                </select>
              </div>
              <div class="create_btn tab">
                <a class="btn hover" href="">
                  <%= image_tag "/common/icon_add.svg" %>
                  プロダクトの追加
                </a>
              </div>
            </div>
          </div>
          <div>
            <div class="item_head flex_container">
              <div></div>
              <div></div>
              <div>名称</div>
              <div>OS</div>
              <div>導入日</div>
              <div>位置情報</div>
            </div>
            <div class="products_content">
              <ul class="products_list">
                <li class="item">
                  <a href="/controlpanel/<%= @bench.id %>">
