$('h2').html("<%= @result %>");

$('.table_name').css({'display':'none'});

result = "<%= @result %>";


if(result == 'ユーザー'){
    $('#user').css({'display':'block'});
} else if(result == '公園'){
    $('#park').css({'display':'block'});
} else if(result == 'ベンチ'){
    $('#bench').css({'display':'block'});
} else if(result == 'お知らせ'){
    $('#news').css({'display':'block'});
} else if(result == 'メディア情報'){
    $('#media').css({'display':'block'});
} else if(result == '写真'){
    $('#picture').css({'display':'block'});
} else if(result == 'イベント'){
    $('#event').css({'display':'block'});
} else if(result == 'プロダクト'){
    $('#product').css({'display':'block'});
} else if(result == 'ショートカット'){
    $('#shortcut').css({'display':'block'});
} else if(result == 'ベンチからのデータ'){
    $('#tmp_data').css({'display':'block'});
} else if(result == 'ベンチへのコマンド'){
    $('#tmp_message').css({'display':'block'});
}